<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RockBoxPlaylistTool">
    <DataTemplate DataType="{x:Type local:MusicViewModel}">
        <local:MusicUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:PlaylistViewModel}">
        <local:PlaylistUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:MainViewModel}">
        <local:MainUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:SongData}">
        <local:SongDataUserControl/>
    </DataTemplate>
</ResourceDictionary>